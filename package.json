{
	"name": "react-stocks",
	"version": "0.1.0",
	"private": false,
	"scripts": {
		"start": "webpack-dev-server --config webpack.config.dev.js --port 3000"
	},
	"engines": {
		"node": ">=8"
	},
	"babel": {
		"presets": [
			"babel-preset-react-app"
		]
	},
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"plugin:import/errors",
			"plugin:import/warnings"
		],
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": 2018,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"env": {
			"browser": true,
			"node": true,
			"es6": true,
			"jest": true
		},
		"rules": {
			"no-debugger": "off",
			"no-console": "off",
			"no-unused-vars": "warn",
			"react/prop-types": "warn"
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"root": true
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"bootstrap": "^4.3.1",
		"immer": "^3.1.3",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-bootstrap": "^1.0.0-beta.9",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.0.1",
		"react-scripts": "3.0.1",
		"redux": "^4.0.4",
		"redux-thunk": "^2.3.0",
		"reselect": "^4.0.0"
	},
	"devDependencies": {
		"@babel/core": "7.3.4",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"babel-preset": "^1.0.15",
		"css-loader": "^3.1.0",
		"cssnano": "^4.1.10",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"eslint": "^6.1.0",
		"eslint-loader": "^2.2.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-react": "^7.14.2",
		"fetch-mock": "^7.3.9",
		"html-webpack-plugin": "^3.2.0",
		"http-server": "^0.11.1",
		"jest": "^24.8.0",
		"json-server": "^0.15.0",
		"node-fetch": "^2.6.0",
		"npm-run-all": "^4.1.5",
		"redux-immutable-state-invariant": "^2.1.0",
		"redux-mock-store": "^1.5.3",
		"rimraf": "^2.6.3",
		"style-loader": "^0.23.1",
		"webpack": "^4.36.1",
		"webpack-bundle": "^0.9.27",
		"webpack-cli": "^3.3.6",
		"webpack-dev-server": "^3.7.2"
	}
}
